<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.tian.server.entity.PlayerPackageEntity" table="player_package" schema="bdm25683027_db">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="playerId">
            <column name="playerId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="count">
            <column name="count" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="goodsId">
            <column name="goodsId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="goodsUuid">
            <column name="goodsUUID" sql-type="bigint(19)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>