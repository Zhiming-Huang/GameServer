<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.tian.server.entity.SkillEntity" table="skill" schema="bdm25683027_db">
        <id name="name">
            <column name="name" sql-type="varchar(128)" length="128"/>
        </id>
        <property name="china">
            <column name="china" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="attack">
            <column name="attack" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="dodge">
            <column name="dodge" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="parry">
            <column name="parry" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="damage">
            <column name="damage" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="force2">
            <column name="force2" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="difficult">
            <column name="difficult" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="level">
            <column name="level" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="attr">
            <column name="attr" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>