<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.tian.server.entity.PlayerInfoEntity" table="player_info" schema="bdm25683027_db">
        <property name="playerId">
            <column name="playerId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="isLine">
            <column name="isLine" sql-type="tinyint(4)" not-null="true"/>
        </property>
        <property name="ip">
            <column name="ip" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="loginTime">
            <column name="loginTime" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="cityName">
            <column name="cityName" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="roomName">
            <column name="roomName" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>