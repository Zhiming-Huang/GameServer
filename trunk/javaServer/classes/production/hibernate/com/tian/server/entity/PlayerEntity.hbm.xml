        </property>
        <property name="kar">
            <column name="kar" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="per">
            <column name="per" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="sta">
            <column name="sta" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="role">
            <column name="role" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="wux">
            <column name="wux" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>